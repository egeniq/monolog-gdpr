  "license": "MIT",
  "require": {
    "monolog/monolog": "^1.17|^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {"Egeniq\\Monolog\\Gdpr\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {"Egeniq\\Monolog\\Gdpr\\": "tests/"}
  }
}
